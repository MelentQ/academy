{{#extend "section"}}
{{#content "heading-content"}}

{{/content}}
{{#content "content"}}
<div class="swiper js-catalog-slider catalog">
  <ul class="swiper-wrapper">
    {{#each data.items}}
    <li class="swiper-slide">
      {{> parts/catalog-card data=this}}
    </li>
    {{/each}}
  </ul>
  <div class="swiper-pagination"></div>
  {{> parts/navigation}}
</div>
{{/content}}
{{/extend}}